System:
  git clone git@github.com:nevdelap/nixos-config.git
  cdc - cd to config.
  mc - edit config.
  nrs - rebuild and switch.
Root & User:
  git clone git@github.com:nevdelap/personal-config.git
User:
  ned:
    rustup default stable
    Clone ned repo.
    cd ned
    cargo build --release
